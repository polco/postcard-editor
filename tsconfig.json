{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "baseUrl": "src",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "docs",
        "jsx": "react",
        "sourceMap": true
    },
    "compileOnSave": true
}
